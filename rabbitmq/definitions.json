{
    "rabbit_version": "3.9.11",
    "rabbitmq_version": "3.9.11",
    "users": [
        {
            "name": "${env:RABBITMQ_USER}",
            "password": "${env:RABBITMQ_PASSWORD}",
            "tags": "administrator"
        }
    ],
    "vhosts": [
        {
            "name": "/"
        }
    ],
    "permissions": [
        {
            "user": "${env:RABBITMQ_USER}",
            "vhost": "/",
            "configure": ".*",
            "write": ".*",
            "read": ".*"
        }
        
    ]
  }